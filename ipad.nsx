001\"\r\n  End If\r\n  mytext = xselectedclass & \",\" & calledfrom\r\n  req=Ajax(\"ajaxvod.php/?xtemp=\" & mytext) \r\n  If req.status=200 Then        'success\r\n    classvod=JSON.parse(req.responseText)    \r\n    maxvod=UBound(classvod)\r\n    i=0\r\n    imgpath=\"http://www.prasit999.com/media/images/\"\r\n    Do While i <= 8 'maxvod\r\n      If i=0 Then\r\n        If i<=maxvod Then\r\n          videodesc1.textContent = classvod[i].detail        \r\n          myImage1.src=imgpath & classvod[i].name & \".jpg\" \r\n        Else          \r\n'          myImage1.src=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 1 Then\r\n        If i<=maxvod Then \r\n          videodesc2.textContent = classvod[i].detail\r\n          myImage2.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage2.src=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 2 Then\r\n        If i<=maxvod Then\r\n          videodesc3.textContent = classvod[i].detail\r\n          myImage3.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage3.src=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 3 Then\r\n        If i<=maxvod Then        \r\n          videodesc4.textContent = classvod[i].detail\r\n          myImage4.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage4.sec=imgpath & \"blank.jpg\"\r\n        End If        \r\n      ElseIf i = 4 Then\r\n        If i<=maxvod Then        \r\n          videodesc5.textContent = classvod[i].detail\r\n          myImage5.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage4.sec=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 5 Then        \r\n        If i<=maxvod Then\r\n          videodesc6.textContent = classvod[i].detail\r\n          myImage6.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage4.sec=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 6 Then        \r\n        If i<=maxvod Then\r\n          videodesc7.textContent = classvod[i].detail\r\n          myImage7.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage4.sec=imgpath & \"blank.jpg\"\r\n        End If\r\n      ElseIf i = 7 Then        \r\n        If i<=maxvod Then\r\n          videodesc8.textContent = classvod[i].detail\r\n          myImage8.src=imgpath & classvod[i].name & \".jpg\"\r\n        Else\r\n'          myImage4.sec=imgpath & \"blank.jpg\"\r\n        End If\r\n      End If\r\n      i = i + 1\r\n    Loop\r\n  End If  \r\nEnd Function\r\n\r\nFunction myImage1_onload()\r\n    pb1.drawImage(myImage1,0,0,140,80)\r\nEnd Sub  \r\nFunction myImage2_onload()\r\n    pb2.drawImage(myImage2,0,0,140,80)\r\nEnd Sub  \r\nFunction myImage3_onload()    \r\n    pb3.drawImage(myImage3,0,0,140,80)\r\nEnd Sub  \r\nFunction myImage4_onload()    \r\n    pb4.drawImage(myImage4,0,0,140,80)\r\nEnd Sub  \r\nFunction myImage5_onload()    \r\n    pb5.drawImage(myImage5,0,0,140,80)\r\nEnd Sub\r\nFunction myImage6_onload()    \r\n    pb6.drawImage(myImage6,0,0,140,80)\r\nEnd Sub\r\nFunction myImage7_onload()    \r\n    pb7.drawImage(myImage7,0,0,140,80)\r\nEnd Sub\r\nFunction myImage8_onload()\r\n    pb8.drawImage(myImage8,0,0,140,80)\r\nEnd Sub\r\n\r\nFunction myImagex_onload()\r\n'  pb1.drawImage(myImage1,0,0,140,80)\r\nEnd Function\r\n \r\nFunction frmistudyvideo_onclick(event)\r\n  xpbclass=event.target.id      'pbvideo clicked\r\n  If Mid(xpbclass,1,Len(xpbclass)-1) = \"pbclass\" Then\r\n    xi=Right(xpbclass,1)-1        'associated array element ->classvod(xi)\r\n    If xi<=maxvod Then\r\n      xselectedvideo = classvod[xi].name\r\n      xselectedvideodesc =classvod[xi].detail\r\n  \r\n      istudyvideo.src = \"http://www.prasit999.com/media/videos/\" & xselectedvideo & \".mp4\"\r\n      txtvideodesc.value = xselectedvideodesc\r\n      istudyvideo.play()\r\n'save onto database\r\n      mytext = xselectedvideo & \",\" & loginname\r\n      req=Ajax(\"ajaxtrsave.php/?xtemp=\" & mytext)\r\n      If req.status=200 Then        'success\r\n        classvod=JSON.parse(req.responseText)    \r\n      End If\r\n    End If\r\n  End If\r\nEnd Function\r\n\nFunction cmdbacktofrmIstudy_onclick()\r\n  fnreset\r\n  If calledfrom = \"istudy\" Then\r\n    ChangeForm(frmIstudy)\r\n  ElseIf calledfrom = \"itutor\" Then\r\n    ChangeForm(frmIstudy)\r\n  ElseIf calledfrom = \"iseminar\" Then\r\n    ChangeForm(frmMain)\r\n  ElseIf calledfrom = \"ishow\" Then\r\n    ChangeForm(frmMain)\r\n  ElseIf calledfrom = \"ientertain\" Then\r\n    ChangeForm(frmMain)\r\n  ElseIf calledfrom = \"iworld\" Then\r\n    ChangeForm(frmMain)    \r\n  End If\nEnd Function\r\n\r\nFunction fnreset()\r\n    istudyvideo.pause()\r\n    istudyvideo.src=\" \"\r\n    istudyvideo.play()\r\n    istudyvideo.pause()\r\n    txtvideodesc.value=\"\"\r\n    pb1.drawImage(myImagex,0,0,140,80)\r\n    pb2.drawImage(myImagex,0,0,140,80)\r\n    pb3.drawImage(myImagex,0,0,140,80)\r\n    pb4.drawImage(myImagex,0,0,140,80)\r\n    pb5.drawImage(myImagex,0,0,140,80)\r\n    pb6.drawImage(myImagex,0,0,140,80)\r\n    pb7.drawImage(myImagex,0,0,140,80)\r\n    pb8.drawImage(myImagex,0,0,140,80)\r\nEnd Function\r\n    ", 
