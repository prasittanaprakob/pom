tus == 200) { //success\n    tempchat=JSON.parse(req.responseText);\n    i=0;\n    xi = UBound(tempchat);\n   while (i <= xi) {\n      xchatfrom = tempchat[i].chatfrom;\n      xchatto = tempchat[i].chatto;\n      xdetail = tempchat[i].detail;\n      xchatdate = tempchat[i].chatdate;\n      xchattime = tempchat[i].chattime;\n      if(Len(xdetail) > 0) {\n        if(xchatfrom == loginname) {\n          txtdisplayarea.value = txtdisplayarea.value  +  xchatdate  +  \" \"  +  xchattime  +  \" >> : \"  +  xdetail  +  Chr(10);\n } else {\n          txtdisplayarea.value = txtdisplayarea.value  +  xchatdate  +  \" \"  +  xchattime  +  \" << : \"  +  xdetail  +  Chr(10);\n        }\n      }\n      i = i + 1;\n     }\n    txtchat.value=\"\";\n //    txtchat.focus()\n  }\nreturn savethefunction_rvar; }\n\ntxtchat.onkeypress = function(kid) { savethefunction_rvar=\"\";\n\nreturn savethefunction_rvar; }\n", 
